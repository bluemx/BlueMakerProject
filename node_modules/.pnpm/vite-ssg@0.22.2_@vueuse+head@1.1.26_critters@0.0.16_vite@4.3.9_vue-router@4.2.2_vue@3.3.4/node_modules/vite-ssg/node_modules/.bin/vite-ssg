#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/fldsmdfr/Documents/GitHub/BlueMakerProject/node_modules/.pnpm/vite-ssg@0.22.2_@vueuse+head@1.1.26_critters@0.0.16_vite@4.3.9_vue-router@4.2.2_vue@3.3.4/node_modules/vite-ssg/bin/node_modules:/Users/fldsmdfr/Documents/GitHub/BlueMakerProject/node_modules/.pnpm/vite-ssg@0.22.2_@vueuse+head@1.1.26_critters@0.0.16_vite@4.3.9_vue-router@4.2.2_vue@3.3.4/node_modules/vite-ssg/node_modules:/Users/fldsmdfr/Documents/GitHub/BlueMakerProject/node_modules/.pnpm/vite-ssg@0.22.2_@vueuse+head@1.1.26_critters@0.0.16_vite@4.3.9_vue-router@4.2.2_vue@3.3.4/node_modules:/Users/fldsmdfr/Documents/GitHub/BlueMakerProject/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/fldsmdfr/Documents/GitHub/BlueMakerProject/node_modules/.pnpm/vite-ssg@0.22.2_@vueuse+head@1.1.26_critters@0.0.16_vite@4.3.9_vue-router@4.2.2_vue@3.3.4/node_modules/vite-ssg/bin/node_modules:/Users/fldsmdfr/Documents/GitHub/BlueMakerProject/node_modules/.pnpm/vite-ssg@0.22.2_@vueuse+head@1.1.26_critters@0.0.16_vite@4.3.9_vue-router@4.2.2_vue@3.3.4/node_modules/vite-ssg/node_modules:/Users/fldsmdfr/Documents/GitHub/BlueMakerProject/node_modules/.pnpm/vite-ssg@0.22.2_@vueuse+head@1.1.26_critters@0.0.16_vite@4.3.9_vue-router@4.2.2_vue@3.3.4/node_modules:/Users/fldsmdfr/Documents/GitHub/BlueMakerProject/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/vite-ssg.js" "$@"
else
  exec node  "$basedir/../../bin/vite-ssg.js" "$@"
fi
