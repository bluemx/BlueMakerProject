{
  "type": "module",
  "private": true,
  "packageManager": "pnpm@8.6.7",
  "scripts": {
    "build": "vite build",
    "dev": "vite --port 3333 --open",
    "lint": "eslint --fix --ext ..js,.vue,.ts .",
