    "@types/nprogress": "^0.2.0",
    "@unocss/eslint-config": "^0.53.6",
    "@vitejs/plugin-vue": "^4.3.4",
    "@vue-macros/volar": "^0.12.3",
    "@vue/test-utils": "^2.4.1",
